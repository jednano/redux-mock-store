{
  "name": "@jedmao/redux-mock-store",
  "version": "2.0.3",
  "description": "A mock store for testing your redux async action creators and middleware",
  "keywords": [
    "react",
    "redux",
    "mock",
    "store",
    "thunk",
    "test"
  ],
  "main": "dist/es2015/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/es2015/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "prebuild": "npm run clean",
    "build": "run-p build:es2015 build:esm",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es6 --target es6 --outDir dist/esm",
    "test": "jest --watch",
    "cover": "jest --coverage",
    "commit": "git-cz",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jedmao/redux-mock-store.git"
  },
  "author": "Jed Mao <jedmao@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jedmao/redux-mock-store/issues"
  },
  "homepage": "https://github.com/jedmao/redux-mock-store#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --coverage"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "git reset"
    ]
  },
  "prettier": {
    "arrowParens": "avoid",
    "proseWrap": "always",
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "lines": 100,
        "functions": 100
      }
    },
    "coverageReporters": [
      "json",
      "lcov"
    ],
    "preset": "ts-jest"
  },
  "dependencies": {
    "@queso/kebab-case": "^1.1.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isplainobject": "^4.0.6",
    "ts-helpers": "^1.1.2"
  },
  "peerDependencies": {
    "redux": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@jedmao/semantic-release-npm-github-config": "^1.0.6",
    "@jedmao/tsconfig": "^0.3.0",
    "@testing-library/react": "^9.1.3",
    "@types/jest": "^24.0.18",
    "@types/lodash.isfunction": "^3.0.6",
    "@types/lodash.isplainobject": "^4.0.6",
    "@types/node": "^12.7.2",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "redux-thunk": "^2.3.0",
    "rimraf": "^3.0.0",
    "semantic-release": "^15.13.21",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  },
  "release": {
    "branch": "master",
    "extends": "@jedmao/semantic-release-npm-github-config"
  }
}
